@model List<Task_Markova.Models.Person>
<table>
    <tbody>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.Name</td>
                <td>@person.Surname</td>
                <td>@person.Age</td>
                <td>@person.Profession</td>
            </tr>
        }
    </tbody>
</table>



